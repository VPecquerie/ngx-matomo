{
  "name": "ngx-matomo",
  "version": "0.1.4",
  "author": "Arnaud Mouronval <arnaud.mouronval@gmail.com>",
  "description": "Matomo (aka. Piwik) web analytics for applications based on Angular 9.",
  "keywords": [
    "matomo",
    "piwik",
    "angular",
    "aot",
    "web analytics"
  ],
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Arnaud73/ngx-matomo.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test-headless": "ng test --codeCoverage=true --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf dist/*",
    "format:write": "prettier projects/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier projects/**/*.{ts,json,md,scss} --list-different",
    "compodoc": "compodoc -c ./.compodocrc.json",
    "copy-license": "copyfiles ./LICENSE ./dist/ngx-matomo",
    "copy-readme": "copyfiles ./README.md ./dist/ngx-matomo",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "release": "standard-version",
    "postinstall": "ngcc"
  },
  "dependencies": {
    "@angular/animations": "~10.1.3",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/core": "~10.1.3",
    "@angular/forms": "~10.1.3",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular-devkit/build-ng-packagr": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@angular/language-service": "~10.1.3",
    "@types/node": "^14.11.2",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.8",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^10.1.2",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.3"
  },
  "standard-version": {}
}
